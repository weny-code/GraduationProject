.zp-loading-more-line-loading-image.data-v-245fe000 {
	margin-right: 8rpx;
	width: 28rpx;
	height: 28rpx;

	-webkit-animation: loading-flower-data-v-245fe000 1s steps(12) infinite;
	        animation: loading-flower-data-v-245fe000 1s steps(12) infinite;

	color: #666666;
}
@-webkit-keyframes loading-flower-data-v-245fe000 {
0% {
		-webkit-transform: rotate(0deg);
		transform: rotate(0deg);
}
to {
		-webkit-transform: rotate(1turn);
		transform: rotate(1turn);
}
}
@keyframes loading-flower-data-v-245fe000 {
0% {
		-webkit-transform: rotate(0deg);
		transform: rotate(0deg);
}
to {
		-webkit-transform: rotate(1turn);
		transform: rotate(1turn);
}
}
.zp-load-more-container.data-v-245fe000 {
	height: 80rpx;
	font-size: 26rpx;

	clear: both;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;

	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
